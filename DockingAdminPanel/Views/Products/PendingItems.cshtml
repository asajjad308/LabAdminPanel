@model IEnumerable<DockingAdminPanel.Models.Products>

@{
    ViewData["Title"] = "PendingItems";
}
<link rel="stylesheet" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.css" />

<script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.js"></script>
<h1>PendingItems</h1>

<div class="card-body">
    <h5 class="card-title">All Dockings</h5>
    <table id="myTable" class="display">
        <thead>
            <tr>
                <th>Address</th>
                <th>Location</th>
                <th>spaceNumber</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.address</td>
                    <td>@item.location</td>
                    <td>@item.spaceNumber</td>
                    <td>
                        <button class="btn btn-primary btn-block" onclick="approve('@item.Id')">Approve</button>
                        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
